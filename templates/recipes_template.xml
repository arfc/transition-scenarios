<recipes>
  <recipe>
    <name>fresh_uox</name>
    <basis>atom</basis>
    {% for key, value in fresh.items() -%}
    <nuclide> <id>{{ key }}</id> <comp>{{ value }}</comp> </nuclide>
    {% endfor -%}
  </recipe>
