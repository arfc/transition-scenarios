<facility>
  <name>{{name}}</name>
  <lifetime>{{lifetime}}</lifetime>
  <config>
    <Reactor>
      <fuel_incommods>  <val>fresh_uox</val> <val>fresh_uox2</val> </fuel_incommods>
      <fuel_inrecipes>  <val>fresh_uox</val> <val>fresh_uox</val> </fuel_inrecipes>
      <fuel_outcommods> <val>spent_uox</val> <val>fuel</val> </fuel_outcommods>
      <fuel_outrecipes> <val>spent_uox</val> <val>avg_recipe</val> </fuel_outrecipes>
      <fuel_prefs> <val>10</val> <val>1</val> </fuel_prefs>


      <pref_change_times><val>{{pref_change_time}}</val> </pref_change_times>
      <pref_change_commods> <val>fresh_uox</val> </pref_change_commods>
      <pref_change_values> <val>0</val> </pref_change_values>

      <cycle_time>18</cycle_time>
      <refuel_time>1</refuel_time>
      <assem_size>{{assem_size}}</assem_size>
      <n_assem_core>{{n_assem_core}}</n_assem_core>
      <n_assem_batch>{{n_assem_batch}}</n_assem_batch>
      <power_cap>{{power_cap}}</power_cap>
      <longitude>{{lon}}</longitude>
      <latitude>{{lat}}</latitude>
    </Reactor>
    
  </config>
</facility>
