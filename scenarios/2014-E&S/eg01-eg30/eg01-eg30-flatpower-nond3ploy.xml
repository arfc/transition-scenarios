                <stream>
                    <info>
                        <mixing_ratio>0.1146</mixing_ratio>
                        <buf_size>2e4</buf_size>
                    </info>
                    <commodities>
                        <item>
                            <commodity>lwrtru</commodity>
                            <pref>1.0</pref>
                        </item>
                        <item>
                            <commodity>frtru</commodity>
                            <pref>1.0</pref>
                        </item>
                        <item>
                            <commodity>moxtru</commodity>
                            <pref>1.0</pref>
