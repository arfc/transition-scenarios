    </config>
</facility>

<facility>
    <name>lwrreprocessing</name>
    <config>
        <Separations>
            <feed_commods>
                <val>lwrstorageout</val>
            </feed_commods>
            <feedbuf_size>1e8</feedbuf_size>
            <throughput>1e8</throughput>
            <leftover_commod>lwrreprocessingwaste</leftover_commod>
            <leftoverbuf_size>1e8</leftoverbuf_size>
            <streams>
                <item>
                    <commod>lwrtru</commod>
                    <info>
                        <buf_size>1e8</buf_size>
                        <efficiencies>
                              <item>
                                <comp>Np</comp>
                                <eff>1.0</eff>
                              </item>
                              <item>
                                <comp>Am</comp>
                                <eff>1.0</eff>
                              </item>
                              <item>
                                <comp>Cm</comp>
                                <eff>1.0</eff>
                              </item>
                              <item>
                                <comp>Pu</comp>
                                <eff>1.0</eff>
                              </item>
                        </efficiencies>
                    </info>
                </item>
                <item>
                    <commod>lwru</commod>
                    <info>
                        <buf_size>1e8</buf_size>
                        <efficiencies>
                            <item>
                                <comp>U</comp>
                                <eff>1.0</eff>
                            </item>
                        </efficiencies>
                    </info>
                </item>
            </streams>
        </Separations>
    </config>
</facility>

