  <facility>
    <name>Sink_HLW</name>
    <config>
      <Sink>
        <in_commods>
          <val>used_uox</val>
        </in_commods>
        <!-- Yucca Mountain Repository -->
        <latitude>36.8286</latitude>
        <longitude>-116.4364</longitude>
      </Sink>
    </config>
  </facility>

  <facility>
    <name>Sink_LLW</name>
    <config>
      <Sink>
        <in_commods>
          <val>tails</val>
        </in_commods>
        <!-- Yucca Mountain Repository -->
        <latitude>36.8286</latitude>
        <longitude>-116.4364</longitude>
      </Sink>
    </config>
  </facility>

  <facility>
    <name>Mine</name>
    <config>
      <Source>
        <outcommod>nat_u</outcommod>
        <outrecipe>nat_u</outrecipe>
        <throughput>1e6</throughput>
        <!-- Cameco Smith Ranch-Highland Mine -->
        <latitude>43.053736</latitude>
        <longitude>-105.685089</longitude>
      </Source>
    </config>
  </facility>

  <facility>
    <name>Mine_mmr</name>
    <config>
      <Source>
        <outcommod>nat_u_mmr</outcommod>
        <outrecipe>nat_u_mmr</outrecipe>
        <throughput>1e6</throughput>
        <!-- Cameco Smith Ranch-Highland Mine -->
        <latitude>43.053736</latitude>
        <longitude>-105.685089</longitude>
